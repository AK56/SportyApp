<p id="notice"><%= notice %></p>

<h1>Welcome to <%= @org.name%></h1>

<% @teams.each do |team| %>
  <div id="team_activity_chart">
    <div class="bolded">Team <%= team.name %> Activity</div>
    <%= line_chart team_activity_chart_path(team), xtitle: "Participation Date", ytitle: "Duration in mins." %>
  </div>
<% end %>
</div>
<div class="clear"></div>
<br>